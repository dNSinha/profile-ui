{
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-transform-modules-commonjs"
    ],
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "env": {
        "test": {
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react"
            ],
            "plugins": [
                "dynamic-import-node",
                "@babel/plugin-transform-runtime", [
                    "@babel/plugin-transform-modules-commonjs"
                ],
                "@babel/plugin-syntax-dynamic-import"
            ]
        }
    }
}
